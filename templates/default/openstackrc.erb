export OS_AUTH_USER=<%= @creds[:auth_user] %>
export OS_AUTH_KEY=<%= @creds[:auth_key] %>
export OS_AUTH_TENANT=<%= @creds[:auth_tenant] %>
export OS_AUTH_URL=<%= @creds[:auth_url] %>
export OS_AUTH_STRATEGY=keystone

# legacy options for novaclient
export NOVA_API_KEY="$OS_AUTH_KEY"
export NOVA_USERNAME="$OS_AUTH_USER"
export NOVA_PROJECT_ID="$OS_AUTH_TENANT"
export NOVA_URL="$OS_AUTH_URL"
export NOVA_VERSION="1.1"
